{
	"name": "iar-arm-asm",
	"displayName": "IAR ARM Assembler",
	"description": "IAR ARM assembly language support for Visual Studio Code",
	"version": "0.2.1",
	"publisher": "paul-fox",
	"icon": "images/icon.png",
	"author": {
		"name": "Paul Fox"
	},
	"categories": [
		"Programming Languages"
	],
	"engines": {
		"vscode": "^0.10.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/pfox89/vscode-iar-asm-arm.git"
	},
	"galleryBanner": {
		"color": "#0084A9",
		"theme": "dark"
	},
	"contributes": {
		"languages": [
			{
				"id": "iar",
				"aliases": [
					"IAR",
					"iar"
				],
				"extensions": [
					".s",
					".S"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "iar",
				"scopeName": "source.iar",
				"path": "./syntaxes/iar.tmlanguage.json"
			}
		],
		"breakpoints": [
			{
				"language": "iar"
			}
		]
	}
}
